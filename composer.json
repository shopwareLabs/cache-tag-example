{
    "name": "swag/cache-tag-example",
    "description": "Example plugin demonstrating the new cache tag system in Shopware 6.7+",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Shopware AG",
            "homepage": "https://www.shopware.com"
        }
    ],
    "homepage": "https://github.com/shopware/SwagCacheTagExample",
    "support": {
        "issues": "https://github.com/shopware/SwagCacheTagExample/issues",
        "docs": "https://developer.shopware.com/docs/guides/plugins/plugins/framework/caching/"
    },
    "keywords": [
        "shopware",
        "shopware6",
        "cache",
        "cache-tags",
        "http-cache",
        "example",
        "plugin"
    ],
    "require": {
        "shopware/core": ">=6.7.0"
