        }
    },
    "extra": {
        "shopware-plugin-class": "Swag\\CacheTagExample\\SwagCacheTagExample",
        "label": {
            "de-DE": "Cache Tag Beispiel",
            "en-GB": "Cache Tag Example"
        },
        "description": {
            "de-DE": "Beispiel-Plugin f√ºr das neue Cache-Tag-System in Shopware 6.7+",
            "en-GB": "Example plugin demonstrating the new cache tag system in Shopware 6.7+"
        }
